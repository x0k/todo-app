    "@mui/lab": "5.0.0-alpha.133",
    "@mui/material": "^5.13.4",
    "@planetscale/database": "^1.7.0",
    "atomic-router": "^0.8.0",
    "atomic-router-react": "^0.8.3",
    "browser-fs-access": "^0.34.1",
    "date-fns": "^2.30.0",
    "drizzle-orm": "^0.26.5",
    "effector": "^22.8.6",
    "effector-react": "^22.5.3",
    "fast-deep-equal": "^3.1.3",
    "history": "^5.3.0",
    "idb": "^7.1.1",
    "memoize-one": "^6.0.0",
    "nanoid": "^4.0.2",
    "notistack": "^3.0.1",
