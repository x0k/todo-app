    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "prettier": "^2.8.8",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vite-plugin-mkcert": "^1.15.0",
    "vite-plugin-pwa": "^0.16.3",
