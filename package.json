{
  "name": "todo-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "drizzle-generate:planetscale": "drizzle-kit generate:mysql --schema ./src/shared/planetscale-schema.ts --out ./drizzle-migrations/planetscale",
    "drizzle-generate": "npm run drizzle-generate:planetscale",
    "hydrate-drizzle-migrations": "node ./scripts/hydrate-drizzle-migrations.mjs",
    "regenerate-drizzle-migrations": "npm run drizzle-generate && npm run hydrate-drizzle-migrations"
  },
